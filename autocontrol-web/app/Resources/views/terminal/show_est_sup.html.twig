{% extends 'default/index_sup.html.twig' %}

{% block body %}
    <legend>Listado de Terminales</legend>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>id</th>
                <th>Estado</th>
                <th>Coordenada</th>
                <th>Autos por Hora</th>
                <th>Energ√≠a Total Generada</th>
            </tr>
        </thead>
        <tbody>
        {% for terminal in terminals %}
            <tr>
                <td>{{ terminal.id }}</td>
                <td>{{ terminal.estado }}</td>
                <td>{{ terminal.coordenada }}</td>
                <td>{{ autos[terminal.id][0] }}</td>
                <td>{{ autos[terminal.id][1] }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
